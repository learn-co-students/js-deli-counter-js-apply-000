// created during first-round interview

function currentLine2(line) {
  var outputArray = [];
  if (line.length === 0) {
    return "The line is currently empty.";
  }
  for (var i = 0; i<line.length; i++) {
    outputArray.push((i+1) + ". " + line[i]);
  }
  return "The line is currently: " + outputArray.join(", ");
}
